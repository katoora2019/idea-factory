<h1>All Ideas</h1>

  <%# </div> %>
<div class='col-5'>
    <div class='list-group'>
<% @ideas.each do |idea| %>
    <div class="form-group">
    <h1><%= link_to idea.title, idea_path(idea) %></h1>
    </div>
    <div class="form-group">
    <p><%= idea.description %></p>
    </div>
    <div class="form-group">
    <small><%= time_ago_in_words(idea.created_at) %></small>
    </div>
<% end %><hr>
